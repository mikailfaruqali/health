{
    "name": "mikailfaruqali/health",
    "description": "A beautiful, responsive Laravel package for creating dynamic health checks and system diagnostics with a stunning UI. Run custom database queries, monitor system health, and troubleshoot issues through an elegant dashboard interface.",
    "keywords": [
        "laravel",
        "health-check",
        "monitoring",
        "diagnostics",
        "troubleshooting",
        "snawbar"
    ],
    "homepage": "https://github.com/mikailfaruqali/health",
    "license": "MIT",
    "authors": [
        {
            "name": "Snawbar",
            "email": "alanfaruq85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": ">=5.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.22 || ^9.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snawbar\\Health\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snawbar\\Health\\HealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
